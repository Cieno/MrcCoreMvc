@model MRCDataLibrary._02_Models.MemberModel

<h1>Delete</h1>

@if (Model is null)
{
    <h4>The requested record was not found.</h4>
}
else
{
    <div class="h4">
        Are you sure you want to delete @Model.FIRST_NAME + @Model.LAST_NAME's information?
    </div>
    <form asp-action="Delete">
        <input asp-for="@Model.MEMBER_ID" type="hidden" />
        <input type="submit" value="Delete" class="btn btn-danger" />
    </form>
}